<?xml version="1.0" encoding="UTF-8"?>
<templates>
    <t t-name="bms.maintainance_object" owl="1">
        <div>
            +++ maintainance_object ++++++
        </div>

        <t t-log="fieldsProps"/>
        <t t-foreach="fieldsProps" t-as="fieldProps" t-key="fieldProps.id">
           <div>
            <Field name="fieldProps.fieldName" record="fieldProps.fieldRecord" fieldInfo="fieldProps.fieldInfo" class="fieldProps.fieldClass" setDirty="fieldProps.setDirty" readonly="0"/>
            </div>
        </t>
        
        <ObjectType maintainance_object_id="maintainance_object_id"/>
    

        <div>
            ----- end maintainance object ---
        </div>
    </t>
</templates>
