<?xml version="1.0" encoding="UTF-8" ?>
<templates>
    <t t-name="bms.oslo_type" owl="1">

        <div class="row m-0 p-0">
            <div class="col-2 m-0 p-0">
                <span class="o_form_label"> object type  </span>
            </div>
            <div class="col-9 m-0 p-0">
                <t t-out="className"/>
            </div>
            <div class="col-1 m-0 p-0  d-flex justify-content-end">
                <button class="btn btn-primary" t-on-click="() => this.changeOtlAndType()">
                    <i class="fa fa-pencil" aria-hidden="true"></i>
                </button>
            </div>
        </div>
        <div>
            <hr></hr>
        </div>
        <div>
            <t t-foreach="attrDefs.widget_attrs"  t-as="widget_attr" t-key="widget_attr.widget_name">
                <t t-component="widget_attr.Component" attr_defs="widget_attr.attr_defs" objectId="objectId" objectTypeId="objectTypeId"/> 
            </t>
        </div>
        <div>

        </div>
    </t>
</templates>