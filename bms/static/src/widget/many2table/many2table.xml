<?xml version="1.0" encoding="UTF-8" ?>
<templates xml:space="preserve">
    <t t-name="bms.Many2ManyTable" owl="1">

        <div> hello world bitch!</div>
        <!-- OTL splitsing -->

        <nav>
            <div class="nav nav-tabs" id="nav-tab-otl" role="tablist">
                <t t-foreach="props.value.records" t-as="record" t-key="record.id">
                    <button t-attf-class="nav-link {{(record_first)? 'active' : ''}}" t-attf-id="nav-otl-{{record_index}}-tab" data-bs-toggle="tab" t-attf-data-bs-target="#nav-otl-{{record_index}}" type="button" role="tab" t-attf-aria-controls="nav-otl-{{record_index}}" t-attf-aria-selected="{{ (record_first)? 'true' : 'false' }}">
                        <t t-out="record.data.otl_id[1]"/>
                    </button>
                </t>
            </div>
        </nav>
        <div class="tab-content" id="nav-tabContent-otl">
            <t t-foreach="props.value.records" t-as="record" t-key="record.id">
                <div t-attf-class="tab-pane fade {{(record_first) ? 'show active' : ''}}" t-attf-id="nav-otl-{{record_index}}" role="tabpanel" t-attf-aria-labelledby="nav-otl-{{record_index}}-tab">
                      <!-- <t t-out="record.data.name"/> -->
                    <t t-log="'name'"/>
                   
                    <t t-set="fieldInfo"  t-value="{'FieldComponent' : record.activeFields.name.FieldComponent}"/>
                     <t t-log="'modifiers'"/>
                     <t t-log="fieldInfo.modifiers"/> 
                     <t t-log="'eval context'"/>
                     <t t-log="record.evalContext"/>

                    <Field name="record.activeFields.name.name" record="record" fieldInfo="fieldInfo" class="record.activeFields.name.FieldComponent.name" setDirty="(isDirty) => this.setDirty(isDirty)" readonly="0"/>

                    <!-- class="getFieldClass(column)" fieldInfo="props.archInfo.fieldNodes[column.name]"  -->
                    <!-- class: _class, fieldInfo, name, record  -->
                    <!-- readonly="props.activeActions?.edit === false and !record.isNew" -->   
                </div>
            </t>
        </div>

        <!-- <Field/> -->

        <!-- fieldtypmel -->
        <div> ------------------------------------</div>

        <t t-log="props"/>

    </t>
</templates>