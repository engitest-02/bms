<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <!-- views -->
  <record id="bms_asset_view_tree" model="ir.ui.view">
    <field name="name">bms.asset.tree</field>
    <field name="model">bms.maintainance_object</field>
    <field name="arch" type="xml">
      <tree>
        <field name="name"/>
        <!-- <field name="lantis_unique_id"/>
        <field name="is_active"/>
        <field name="is_asset"/> -->
      </tree>
    </field>
  </record>

  <!-- actions -->
  <record id="bms_asset_action" model="ir.actions.act_window">
    <field name="name">BMS asset</field>
    <field name="res_model">bms.maintainance_object</field>
    <field name="view_mode">tree</field>
  </record>

  <!-- menuitems -->
  <menuitem id="bms_root" name="BMS"/>
  <menuitem id="bms_asset" parent="bms_root" name="Asset" groups="base.group_user"  sequence ="10" action="bms_asset_action"/>


</odoo>