  <record id="bms_maintainance_object_view_form" model="ir.ui.view">
    <field name="name">bms.maintainance_object.form</field>
    <field name="model">bms.maintainance_object</field>
    <field name="arch" type="xml">
      <form string="Maintainance object" js_class="bms_form_with_decompo" duplicate="1">
        <group >
          <group>
            <field name="name" />
            <field name="mo_semantic_id"/>
            <field name="import_source"/>
          </group>
          <group>
            <field name="mo_id"/>
            <field name="awv_id" readonly="1"/>
            <field name="import_id"/>
          </group>
        </group>
        <notebook>
          <page string="OTL AWV">
            <group col="12">
              <group colspan="3">
                <field name="awv_type_not_found" attrs="{'invisible': [('has_object_type_ids', '=', True)]}"/>
              </group>
              <group colspan="6">
                <field name="bo_temporary_type" attrs="{'invisible': [('awv_type_not_found', '=', False)]}"/>
              </group>
            </group>
            <field name="has_object_type_ids" invisible="1"/>
            <field name="object_type_ids" widget="object_type_notebook" attrs="{'invisible': [('awv_type_not_found', '=', True)]}">
              <tree >
                <field name="name"/>
                <field name="definition"/>
                <field name="otl_id"/>
                <field name="otl_type_internal_id"/>
              </tree>
            </field>
          </page>
          <page string="Decomposition">
            <field name="decomposition_ids">
              <tree>
                <field name="object_id" invisible="1"/>
                <field name="parent_object_id_id"/>
                <field name="parent_object_id"/>
                <field name="decomposition_type_id"/>
              </tree>
            </field>
          </page>
          <page string="Management">
            <group col="12" string="Ownership">
              <group colspan="6">
                <field name="owner_id"/>
                <field name="ownership_doc" widget="url" />
              </group>
              <group colspan="6">
                <field name="owner_contact"/>
              </group>
            </group>
            <newline/>
            <group col="12" string="Management">
              <group colspan="12">
                <field name="is_managing_level"/>

              </group>
              <newline/>
              <group colspan="6">
                <field name="managing_org_id" attrs="{'invisible': [('is_managing_level', '=', False)]}"/>
                <field name="inherited_managing_org_id" attrs="{'invisible': [('is_managing_level', '=', True)]}"/>
                <field name="contract_mgr_lantis" attrs="{'invisible': [('is_managing_level', '=', False)]}"/>
                <field name="inherited_contract_mgr_lantis" attrs="{'invisible': [('is_managing_level', '=', True)]}"/>
                <field name="maintainance_mgr_lantis" attrs="{'invisible': [('is_managing_level', '=', False)]}"/>
                <field name="inherited_maintainance_mgr_lantis" attrs="{'invisible': [('is_managing_level', '=', True)]}"/>
                <field name="technical_mgr_lantis" attrs="{'invisible': [('is_managing_level', '=', False)]}"/>
                <field name="inherited_technical_mgr_lantis" attrs="{'invisible': [('is_managing_level', '=', True)]}"/>
              </group>
              <group colspan="6">
                <field name="managing_org_contact" attrs="{'invisible': [('is_managing_level', '=', False)]}"/>
                <field name="inherited_managing_org_contact" attrs="{'invisible': [('is_managing_level', '=', True)]}"/>

                <field name="contract_mgr_contractor" attrs="{'invisible': [('is_managing_level', '=', False)]}"/>
                <field name="inherited_contract_mgr_contractor" attrs="{'invisible': [('is_managing_level', '=', True)]}"/>
                <field name="maintainance_mgr_contractor" attrs="{'invisible': [('is_managing_level', '=', False)]}"/>
                <field name="inherited_maintainance_mgr_contractor" attrs="{'invisible': [('is_managing_level', '=', True)]}"/>
                <field name="technical_mgr_contractor" attrs="{'invisible': [('is_managing_level', '=', False)]}"/>
                <field name="inherited_technical_mgr_contractor" attrs="{'invisible': [('is_managing_level', '=', True)]}"/>
              </group>
            </group>
            <newline/>
            <group string="Handover events">
              
              <field name="event_handover_ids">
                <tree readonly="1">
                  <field name="handover_date"/>
                  <field name="handover_type"/>
                  <field name="transferring_org_id"/>
                  <field name="receiving_org_id"/>
                  <field name="comment"/>
                  <field name ="object_ids" invisible="1"/>
                    <button type="object" name="assign_to_children" string="Assign to children" 
                            confirm="You cannot undo this action. This event will be assigned to all its (grand...)children."
                            class="btn-primary"
                            />
                </tree>
              </field>
            </group>
          </page>
          <page string="GIS">
            <group string="Coordinaten">
              <group>
                <field name="loc_pt_wgs84_x" readonly="1" />
                <field name="loc_pt_wgs84_y" readonly="1"/>
                <field name="loc_pt_wgs84_z" readonly="1"/>
              </group>
              <group>
                <field name="loc_pt_wgs84_source" readonly="1"/>
                <field name="loc_pt_wgs84_precision" readonly="1"/>
              </group>
            </group>
            <group string="Weg &#38; Verkeer">
                <field name="loc_ident" readonly="1"/>
                <field name="loc_ref_milestone" readonly="1"/>
                <field name="loc_ref_distance" readonly="1"/>
            </group>
          
          </page>
        </notebook>
      </form>
    </field>
  </record>

